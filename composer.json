{
  "name": "kirkaracha/whens-my-ferry",
  "description": "Source code for When's My Ferry web site",
  "type": "project",
  "license": "MIT",
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "authors": [
    {
      "name": "Kirk Franklin",
      "email": "kirk@magneato.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "WhensMyFerry\\": "src/"
    }
  },
  "require": {
    "php": "^7.1",
    "doctrine/orm": "^2.6",
    "filp/whoops": "^2.3",
    "league/container": "^3.2",
    "nikic/fast-route": "^1.3",
    "symfony/http-foundation": "^4.2",
    "twig/twig": "^2.6",
    "vlucas/phpdotenv": "^3.1"
  },
  "require-dev": {
    "codedungeon/phpunit-result-printer": "^0.24.1",
    "friendsofphp/php-cs-fixer": "^2.14",
    "fzaninotto/faker": "^1.8",
    "nunomaduro/collision": "^2.1",
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "^3.4"
  }
}
